{
  "type": "FILE",
  "name": "file-data-provider",
  "syntax": {
  },
  "attributes": [
    {
      "name": "cacheEnable",
      "displayName": "开启缓存",
      "type": "bool",
      "required": false,
      "defaultValue": true,
      "description": "开启后会将查询的数据保存到内部数据库"
    },
    {
      "name": "cacheTimeout",
      "displayName": "缓存过期时间",
      "type": "string",
      "required": false,
      "defaultValue": "30",
      "description": "缓存的过期时间（分钟）。设置为 -1 表示不过期，当修改数据源后，仍然会清空缓存。"
    },
    {
      "name": "schemas",
      "type": "files",
      "displayName": "元数据",
      "key": "tableName",
      "extract": false,
      "children": [
        {
          "name": "tableName",
          "displayName": "表名",
          "required": true,
          "defaultValue": "",
          "type": "string"
        },
        {
          "name": "format",
          "displayName": "格式",
          "required": true,
          "defaultValue": "",
          "type": "string",
          "options": [
            "XLSX",
            "CSV"
          ]
        },
        {
          "name": "path",
          "displayName": "文件上传",
          "required": true,
          "defaultValue": "",
          "type": "string",
          "description": "文件路径，上传后自动生成"
        },
        {
          "name": "columns",
          "displayName": "列名",
          "defaultValue": "",
          "type": "schema"
        }
      ]
    }
  ]
}
